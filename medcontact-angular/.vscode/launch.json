{
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
  
      {
  
        "name": "Edge",
        "type": "msedge",
        "request": "launch",
        "resolveSourceMapLocations": [
          "${workspaceFolder}/medcontact"],
        "webRoot": "${workspaceFolder}/medcontact",
        "url": "http://localhost:4200/"
      },
  
      {
       "name": "Chrome",
       "type": "chrome",
       "request": "launch",
       "resolveSourceMapLocations": [
        "${workspaceFolder}/medcontact/src/app"],
       "webRoot": "${workspaceFolder}",
       "url": "http://localhost:4200/"
    },
      {
        "name": "Firefox",
        "type": "firefox",
        "request": "launch",
        "reAttach": true,
        "webRoot": "${workspaceFolder}",
        "pathMappings": [{"url": "webpack:///src/", "path": "${webRoot}/src/"}],
        "url": "http://localhost:4200/",
    },
      {
        "name": "ng test",
        "type": "msedge",
        "request": "launch",
        "preLaunchTask": "npm: test",
        "url": "http://localhost:9876/debug.html"
      }
    ]
  } 